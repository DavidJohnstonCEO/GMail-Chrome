{
  "name": "LD Engine",
  "version": "0.1",
  "description": "Pulls keywords out of GMail messages and adds related messages to sidebar.",
  "permissions": [
  ],
  "manifest_version": 2,
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://mail.google.com/*"],
      "js" : ["jquery-1.8.2.min.js","jquery.dotdotdot-1.5.1.js","jsrender.js","jquery.views.js","jquery.observable.js","ldengine.js"],
      "css" : ["ldengine.css"]
    }
  ],
  "web_accessible_resources": [
    "sidebar.tmpl"
  ]
}
